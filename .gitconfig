[init]
	defaultBranch = main
[user]
	email = tolecnal@tolecnal.net
	name = Jostein Elvaker Haande
	signingkey = ~/.ssh/id_ed25519.pub
[pull]
	rebase = true
[gpg]
	format = ssh
[commit]
	gpgsign = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
